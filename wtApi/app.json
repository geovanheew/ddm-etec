{
  "expo": {
    "name": "wtApi",
    "slug": "wtApi",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/wtIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.geovanheew.wtapi",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/wtIcon.png"
    }
  }
}
